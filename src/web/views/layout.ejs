<!doctype html>
<html>
<head>
  <meta charset="utf-8">
  <meta name="viewport" content="width=device-width, initial-scale=1">
  <title>Jarvis Admin</title>
  <style>
    body{font-family:system-ui,Arial,sans-serif;margin:0;background:#0f1115;color:#e6e6e6}
    header,footer{padding:12px 20px;background:#161a22}
    main{padding:20px}
    a{color:#7dc0ff;text-decoration:none;margin-right:12px}
    input,textarea,select,button{background:#11151b;color:#e6e6e6;border:1px solid #2a3240;border-radius:6px;padding:8px}
    form{display:grid;gap:8px;max-width:600px}
    table{width:100%;border-collapse:collapse;margin-top:12px}
    th,td{border-bottom:1px solid #2a3240;padding:8px;text-align:left}
    .row{display:flex;gap:12px;align-items:center;flex-wrap:wrap}
  </style>
  </head>
  <body>
    <header class="row">
      <div>Jarvis Admin</div>
      <nav>
        <a href="/">Dashboard</a>
        <a href="/agents">Agentes</a>
        <a href="/providers">Provedores</a>
        <a href="/library">Biblioteca</a>
      </nav>
    </header>
    <main>
      <%- body %>
    </main>
    <footer>
      <small>v1</small>
    </footer>
  </body>
</html>